{
  "build": {
    "command": "npm run build",
    "publish": "dist",
    "environment": {
      "NODE_VERSION": "18",
      "NODE_OPTIONS": "--max-old-space-size=4096"
    },
    "wrangler": {
      "entry-point": "index.js",
      "build-command": "npm run build"
    }
  },
  "framework": null,
  "routes": [
    {
      "source": "/*",
      "destination": "/index.html",
      "status": 404
    }
  ]
}